Page {
  height: 100%;
  width: 100%;
}
Page .msgAddress {
  height: 100%;
  width: 100%;
  background: radial-gradient(#f1f2f2 45%, #dbe7ea 100%);
  overflow: hidden;
}
Page .msgAddress .left_title {
  position: absolute;
  left: 32rpx;
  height: 45px;
  width: 202rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
Page .msgAddress .left_title .left {
  height: 44rpx;
  width: 24rpx;
}
Page .msgAddress .left_title .title {
  font-size: 40rpx;
  color: #2b3e43;
  font-family: pingfang;
  font-weight: 400;
}
Page .msgAddress .tab_box {
  height: 36%;
  width: 686rpx;
  position: absolute;
  top: 13%;
  left: 32rpx;
}
Page .msgAddress .tab_box .tab_item {
  width: 686rpx;
  height: 21%;
  background: #ffffff;
  box-shadow: 0rpx 4rpx 40rpx 0rpx rgba(0, 0, 0, 0.02);
  border-radius: 32rpx;
  margin-bottom: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
Page .msgAddress .tab_box .tab_item .tab_title {
  margin-left: 36rpx;
  font-size: 36rpx;
  font-weight: 400;
  color: #2b3e43;
  width: 100%;
}
Page .msgAddress .tab_box .tab_item .elips {
  width: 75%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
Page .msgAddress .tab_box .tab_item .right {
  height: 36rpx;
  width: 20rpx;
  margin-right: 32rpx;
  transition: 0.3s;
}
Page .msgAddress .tab_box .tab_item .right_active {
  transform: rotate(90deg);
  transition: 0.3s;
}
Page .msgAddress .tab_box .picker {
  width: 686rpx;
  height: 21%;
  box-shadow: 0rpx 4rpx 40rpx 0rpx rgba(0, 0, 0, 0.02);
  border-radius: 32rpx;
  margin-bottom: 24rpx;
  background: #ffffff;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
Page .msgAddress .tab_box .picker .picker_box {
  background: blue;
  height: 100%;
  width: 686rpx;
  background: #ffffff;
  box-shadow: 0rpx 4rpx 40rpx 0rpx rgba(0, 0, 0, 0.02);
  border-radius: 32rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
Page .msgAddress .tab_box .picker .picker_box .tab_title {
  margin-left: 36rpx;
  font-size: 36rpx;
  font-weight: 400;
  color: #2b3e43;
}
Page .msgAddress .tab_box .picker .picker_box .right {
  height: 36rpx;
  width: 20rpx;
  margin-right: 32rpx;
  transition: 0.3s;
}
Page .msgAddress .tab_box .picker .picker_box .right_active {
  transform: rotate(90deg);
  transition: 0.3s;
}
Page .msgAddress .tab_box .remind {
  margin-top: 32rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #849ca3;
}
Page .msgAddress .blocks {
  height: 238rpx;
  width: 224rpx;
  position: absolute;
  left: 32rpx;
  bottom: 29%;
}
Page .msgAddress .bg {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 23%;
}
Page .msgAddress .allReady {
  width: 268rpx;
  height: 96rpx;
  background: #27d4a0;
  border-radius: 200rpx 0rpx 0rpx 200rpx;
  position: absolute;
  right: 0;
  bottom: 19%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
Page .msgAddress .allReady .rightHand {
  height: 44rpx;
  width: 42rpx;
  margin-left: 34rpx;
}
Page .msgAddress .allReady .allReady_text {
  font-size: 36rpx;
  font-weight: 500;
  color: #ffffff;
  margin-right: 32rpx;
}

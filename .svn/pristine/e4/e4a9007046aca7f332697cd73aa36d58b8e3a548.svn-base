Page {
  height: 100%;
  width: 100%;
  overflow: hidden;
}
Page .label_wrap {
  height: 100%;
  width: 100%;
  background: linear-gradient(to top, #304352, #d7d2cc);
  overflow: hidden;
}
Page .label_wrap .detailBox {
  padding: 0 32rpx;
  height: 88.8%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 10rpx;
}
Page .label_wrap .detailBox .table {
  height: 80%;
  width: 100%;
  margin-top: 30rpx;
}
Page .label_wrap .detailBox .table .table_header {
  width: 100%;
  height: 40rpx;
  display: flex;
  align-items: center;
  color: #2980B9;
  font-weight: 550;
  font-size: 34rpx;
}
Page .label_wrap .detailBox .table .table_header .creator {
  text-align: center;
  width: 30%;
}
Page .label_wrap .detailBox .table .table_header .label_name {
  text-align: center;
  width: 50%;
}
Page .label_wrap .detailBox .table .table_header .operation {
  text-align: center;
  width: 20%;
}
Page .label_wrap .detailBox .table .table_line {
  margin: 24rpx 0;
  height: 2rpx;
  width: 100%;
  background: #ccc6c0;
  z-index: 10;
}
Page .label_wrap .detailBox .table .table_detail {
  height: calc(100% - 106rpx);
  width: 100%;
  overflow-y: scroll;
}
Page .label_wrap .detailBox .table .table_detail .item_box .item {
  display: flex;
  height: 70rpx;
  align-items: center;
  font-size: 30rpx;
}
Page .label_wrap .detailBox .table .table_detail .item_box .item .item_creator {
  text-align: center;
  width: 30%;
  display: flex;
  align-items: center;
  justify-content: center;
}
Page .label_wrap .detailBox .table .table_detail .item_box .item .item_creator .avator {
  height: 70rpx;
  width: 70rpx;
  border-radius: 50%;
}
Page .label_wrap .detailBox .table .table_detail .item_box .item .item_creator .creator_name {
  margin-left: 20rpx;
}
Page .label_wrap .detailBox .table .table_detail .item_box .item .item_label {
  text-align: center;
  width: 50%;
}
Page .label_wrap .detailBox .table .table_detail .item_box .item .item_operation {
  width: 20%;
  display: flex;
}
Page .label_wrap .detailBox .table .table_detail .item_box .item .item_operation .icon-tianxie_box {
  color: white;
  height: 42rpx;
  background: white;
  width: 42rpx;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-content: center;
  margin-top: 5rpx;
}
Page .label_wrap .detailBox .table .table_detail .item_box .item .item_operation .icon-tianxie_box .icon-tianxie {
  font-size: 52rpx;
  color: #409EFF;
  margin-top: -5rpx;
}
Page .label_wrap .detailBox .table .table_detail .item_box .item .item_operation .icon-lajitong_box {
  color: white;
  height: 54rpx;
  background: #d62828;
  width: 54rpx;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-content: center;
  margin-left: 30rpx;
}
Page .label_wrap .detailBox .table .table_detail .item_box .item .item_operation .icon-lajitong_box .icon-lajitong {
  font-size: 28rpx;
  margin-top: 12rpx;
  margin-left: 2rpx;
}
Page .label_wrap .detailBox .table .table_detail .item_box .inner_line {
  margin: 24rpx 0;
  height: 2rpx;
  width: 100%;
  background: #ccc6c0;
  z-index: 10;
}
Page .label_wrap .detailBox .write_box {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  height: 300rpx;
  width: 500rpx;
  z-index: -10;
  background: white;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  animation-timing-function: ease-out;
  animation-fill-mode: both;
  opacity: 0;
  transform: scale(0.5);
}
Page .label_wrap .detailBox .write_box .title {
  font-size: 32rpx;
  height: 40%;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
Page .label_wrap .detailBox .write_box .input_box {
  height: 19%;
  border: 1rpx solid rgba(0, 0, 0, 0.3);
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding-left: 10rpx;
}
Page .label_wrap .detailBox .write_box .handle_box {
  height: 30%;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
}
Page .label_wrap .detailBox .write_box .handle_box .qx_btn {
  height: 100%;
  width: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: red;
}
Page .label_wrap .detailBox .write_box .handle_box .cut_line {
  height: 50%;
  width: 2rpx;
  background: rgba(0, 0, 0, 0.3);
}
Page .label_wrap .detailBox .write_box .handle_box .qr_btn {
  height: 100%;
  width: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #1a2980;
}
Page .label_wrap .detailBox .write_box_active {
  z-index: 10;
  opacity: 1;
  transform: scale(1);
  transition: all 0.3s ease;
}
Page .label_wrap .detailBox .publish_btn {
  width: 50%;
  height: 80rpx;
  margin-bottom: 20rpx;
  border-radius: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  letter-spacing: 2rpx;
  background: rgba(0, 129, 255, 0.8);
  color: white;
  margin-top: 40rpx;
}
Page .label_wrap .detailBox .publish_btn:active {
  opacity: 0.8;
}

Page {
  height: 100%;
  width: 100%;
}
Page .todolist_wrap {
  width: 100%;
  height: 100%;
  background: #FAF9FE;
  overflow: hidden;
}
Page .todolist_wrap .title_box {
  margin-left: 32rpx;
  height: 45px;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
Page .todolist_wrap .title_box .progress_title {
  font-size: 38rpx;
  color: #2b3e43;
  font-family: pingfang;
  font-weight: 400;
  letter-spacing: 2rpx;
}
Page .todolist_wrap .calendar {
  width: 100%;
  height: 50%;
  overflow: hidden;
}
Page .todolist_wrap .calendar .tit {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  color: #2A2A2A;
  padding: 3.6% 0;
}
Page .todolist_wrap .calendar .tit .current {
  margin: 0 60rpx;
}
Page .todolist_wrap .calendar .content {
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
  padding: 0 3.6%;
  height: 75%;
}
Page .todolist_wrap .calendar .content .day {
  width: 98.57rpx;
  height: 12%;
  line-height: 12%;
  text-align: center;
  flex-shrink: 0;
  font-size: 32rpx;
  color: #14213d;
  margin: 1.2% 0;
  display: flex;
  justify-content: center;
  align-items: center;
  /* background: rgb(224, 199, 199); */
}
Page .todolist_wrap .calendar .content .day .date {
  border-radius: 50%;
  height: 60rpx;
  width: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  position: relative;
}
Page .todolist_wrap .calendar .content .day .date .mask_box {
  position: absolute;
  width: 70%;
  height: 10rpx;
  bottom: -12rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
Page .todolist_wrap .calendar .content .day .date .mask_box .task_0 {
  height: 16rpx;
  width: 16rpx;
  background: #FDE8B9;
  border-radius: 50%;
}
Page .todolist_wrap .calendar .content .day .date .mask_box .task_1 {
  height: 16rpx;
  width: 16rpx;
  background: #F9BC51;
  border-radius: 50%;
}
Page .todolist_wrap .calendar .content .day .date .mask_box .task_2 {
  height: 16rpx;
  width: 16rpx;
  background: #F6A23F;
  border-radius: 50%;
}
Page .todolist_wrap .calendar .content .day .date .mask_box .task_3 {
  height: 16rpx;
  width: 16rpx;
  background: #F48029;
  border-radius: 50%;
}
Page .todolist_wrap .calendar .content .day .date .mask_box .task_4 {
  height: 16rpx;
  width: 16rpx;
  background: #E1E1EB;
  border-radius: 50%;
}
Page .todolist_wrap .calendar .content .day .date .mask_box .event {
  height: 16rpx;
  width: 16rpx;
  background: #97ABFC;
  border-radius: 50%;
}
Page .todolist_wrap .calendar .content .day .isYours {
  border: 6rpx solid #e76f51;
}
Page .todolist_wrap .calendar .content .day .today {
  font-weight: 700;
  border: 4rpx dashed #CC8285;
}
Page .todolist_wrap .calendar .content .week {
  width: 98.57rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  flex-shrink: 0;
  font-size: 28rpx;
  color: #2A2A2A;
  margin: 6rpx 0;
  font-weight: 700;
}
Page .todolist_wrap .calendar .content .gray {
  color: #999;
}

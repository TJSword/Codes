Page {
  height: 100%;
  width: 100%;
  overflow: hidden;
}
Page .demand_wrap {
  height: 100%;
  width: 100%;
  overflow: hidden;
  background: linear-gradient(to top, #304352, #d7d2cc);
}
Page .demand_wrap .detailBox {
  padding: 0 32rpx;
  height: 88.8%;
  overflow: hidden;
}
Page .demand_wrap .detailBox .title {
  width: 100%;
  text-align: center;
  font-size: 44rpx;
  font-weight: 700;
  margin-top: 10rpx;
}
Page .demand_wrap .detailBox .pro_title_box {
  display: flex;
  align-items: center;
  margin-top: 30rpx;
  padding: 0 30rpx;
}
Page .demand_wrap .detailBox .pro_title_box .icon-xiangmu {
  color: #2980b9;
  font-size: 48rpx;
}
Page .demand_wrap .detailBox .pro_title_box .pro_title {
  margin-left: 20rpx;
}
Page .demand_wrap .detailBox .label_rp_box {
  display: flex;
  align-items: center;
  margin-top: 30rpx;
  padding: 0 30rpx;
}
Page .demand_wrap .detailBox .label_rp_box .label_box {
  width: 60%;
  display: flex;
  align-items: center;
}
Page .demand_wrap .detailBox .label_rp_box .label_box .icon-icon_favorite {
  color: #2980b9;
  font-size: 50rpx;
}
Page .demand_wrap .detailBox .label_rp_box .label_box .label {
  margin-left: 20rpx;
}
Page .demand_wrap .detailBox .label_rp_box .commission_box {
  display: flex;
  align-items: center;
  width: 35%;
}
Page .demand_wrap .detailBox .label_rp_box .commission_box .commission_icon {
  height: 50rpx;
  width: 50rpx;
  border-radius: 12rpx;
  background: #2980b9;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
  font-size: 30rpx;
}
Page .demand_wrap .detailBox .label_rp_box .commission_box .commission_num {
  margin-left: 20rpx;
}
Page .demand_wrap .detailBox .label_rp_box .rp_box {
  display: flex;
  width: 40%;
  align-items: center;
}
Page .demand_wrap .detailBox .label_rp_box .rp_box .rp_icon {
  height: 50rpx;
  width: 50rpx;
  border-radius: 12rpx;
  background: #2980b9;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
  font-size: 20rpx;
}
Page .demand_wrap .detailBox .label_rp_box .rp_box .rp {
  margin-left: 20rpx;
}
Page .demand_wrap .detailBox .label_rp_box .rp_box .rp .tip {
  font-size: 22rpx;
  white-space: nowrap;
}
Page .demand_wrap .detailBox .content_box {
  display: block;
  padding: 0 30rpx;
  color: #2f3e46;
  font-size: 28rpx;
}
Page .demand_wrap .detailBox .time_box {
  display: flex;
  align-items: center;
  padding: 0 30rpx;
}
Page .demand_wrap .detailBox .time_box .icon-lipeishijian {
  color: #2980b9;
  font-size: 50rpx;
}
Page .demand_wrap .detailBox .time_box .time_state {
  display: flex;
  margin-left: 20rpx;
  color: #212020;
  align-items: center;
}
Page .demand_wrap .detailBox .time_box .time_state .state {
  padding-right: 15rpx;
  border-right: 1rpx solid #212020;
  white-space: nowrap;
}
Page .demand_wrap .detailBox .time_box .time_state .time {
  margin-left: 15rpx;
  white-space: nowrap;
}
Page .demand_wrap .detailBox .pm_io_box {
  display: flex;
  padding: 0 30rpx;
  align-items: center;
  margin-top: 30rpx;
}
Page .demand_wrap .detailBox .pm_io_box .pm_box {
  display: flex;
  align-items: center;
  width: 60%;
}
Page .demand_wrap .detailBox .pm_io_box .pm_box .pm_icon {
  height: 50rpx;
  width: 50rpx;
  border-radius: 12rpx;
  background: #2980b9;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
  font-size: 20rpx;
}
Page .demand_wrap .detailBox .pm_io_box .pm_box .pm_avator {
  margin-left: 20rpx;
  height: 70rpx;
  width: 70rpx;
  border-radius: 50%;
}
Page .demand_wrap .detailBox .pm_io_box .pm_box .pm_name {
  margin-left: 20rpx;
}
Page .demand_wrap .detailBox .pm_io_box .io_box {
  display: flex;
  align-items: center;
  width: 40%;
}
Page .demand_wrap .detailBox .pm_io_box .io_box .io_icon {
  height: 50rpx;
  width: 50rpx;
  border-radius: 12rpx;
  background: #2980b9;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
  font-size: 20rpx;
}
Page .demand_wrap .detailBox .pm_io_box .io_box .io_avator {
  margin-left: 20rpx;
  height: 70rpx;
  width: 70rpx;
  border-radius: 50%;
}
Page .demand_wrap .detailBox .pm_io_box .io_box .io_name {
  margin-left: 20rpx;
  white-space: nowrap;
}
Page .demand_wrap .detailBox .progress_box {
  display: flex;
  padding: 10rpx 30rpx;
  align-items: center;
  justify-content: center;
}
Page .demand_wrap .detailBox .progress_box .progress_item {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  margin-right: 20px;
  width: 33%;
  color: #003049;
}
Page .demand_wrap .detailBox .progress_box .progress_item::before {
  content: "";
  display: block;
  position: absolute;
  height: 0px;
  width: 80%;
  border-bottom: 2rpx solid #003049;
  left: -50%;
  top: 30rpx;
  z-index: 0;
}
Page .demand_wrap .detailBox .progress_box .progress_item:first-child::before {
  display: none;
}
Page .demand_wrap .detailBox .progress_box .progress_item:nth-child(2) .item_name {
  margin-top: 20rpx;
  font-size: 24rpx;
  padding: 4rpx 8rpx;
  border: 1rpx solid #2980b9;
  background: #2980b9;
  color: white;
  margin-bottom: -9rpx;
}
Page .demand_wrap .detailBox .progress_box .progress_item .icon-sanjiaotop {
  font-size: 62rpx;
  margin-bottom: -8rpx;
}
Page .demand_wrap .detailBox .progress_box .progress_item .icon-yuanquan {
  font-size: 50rpx;
}
Page .demand_wrap .detailBox .progress_box .progress_item .icon-duihao {
  font-size: 46rpx;
  margin-bottom: 2rpx;
}
Page .demand_wrap .detailBox .progress_box .progress_item .item_name {
  margin-top: 20rpx;
  font-size: 24rpx;
}
Page .demand_wrap .detailBox .progress_box .choose {
  color: #fbbd08;
}
Page .demand_wrap .detailBox .progress_box .choose::before {
  content: "";
  display: block;
  position: absolute;
  height: 0px;
  width: 80%;
  border-bottom: 2rpx solid #fbbd08;
  left: -50%;
  top: 30rpx;
  z-index: 0;
}
Page .demand_wrap .detailBox .progress_box .choose:nth-child(2) .item_name {
  margin-top: 20rpx;
  font-size: 24rpx;
  padding: 0;
  border: 0;
  background: transparent;
  margin-bottom: -9rpx;
  color: #fbbd08;
}
Page .demand_wrap .detailBox .comment_box {
  padding: 0 30rpx;
  margin-top: 30rpx;
  color: #011627;
}
Page .demand_wrap .detailBox .comment_box .rp_result_box {
  display: flex;
  align-items: center;
}
Page .demand_wrap .detailBox .comment_box .rp_result_box .icon-shenchajieguo {
  color: #097dc0e0;
  font-size: 42rpx;
}
Page .demand_wrap .detailBox .comment_box .rp_result_box .rp_result {
  margin-left: 20rpx;
}
Page .demand_wrap .detailBox .comment_box .comment {
  margin-top: 10rpx;
}

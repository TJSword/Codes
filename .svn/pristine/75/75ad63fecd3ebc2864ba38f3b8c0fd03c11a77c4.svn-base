Page {
  height: 100%;
  width: 100%;
  overflow: hidden;
}
Page .voteStart_wrap {
  height: 100%;
  width: 100%;
  background: linear-gradient(to top, #304352, #d7d2cc);
  overflow: hidden;
}
Page .voteStart_wrap .tips {
  height: 86.8%;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #2f3e46;
  font-size: 30rpx;
}
Page .voteStart_wrap .detailBox {
  padding: 0 32rpx;
  height: 88.8%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
}
Page .voteStart_wrap .detailBox .label_box {
  height: 9%;
  width: 90%;
  position: relative;
  margin-top: 15rpx;
  overflow: hidden;
  z-index: 50;
  transition: all 0.3s ease-out;
}
Page .voteStart_wrap .detailBox .label_box .title {
  position: absolute;
  bottom: 20rpx;
  font-size: 36rpx;
  left: 0;
  transition: all 0.3s ease-out;
  color: #343a40;
}
Page .voteStart_wrap .detailBox .label_box .title_on {
  font-size: 30rpx;
  color: #006494;
  transition: all 0.3s ease-out;
  transform: translateY(-50rpx);
}
Page .voteStart_wrap .detailBox .label_box .label_input {
  height: 50%;
  width: 100%;
  position: absolute;
  bottom: 7%;
  z-index: 20;
  font-size: 36rpx;
  display: flex;
  align-items: center;
}
Page .voteStart_wrap .detailBox .label_box .label_input .tip {
  font-size: 26rpx;
  margin-left: 20rpx;
  text-decoration: underline;
}
Page .voteStart_wrap .detailBox .label_box .label_input .rp_info {
  font-size: 26rpx;
  margin-left: 20rpx;
  color: #495057;
}
Page .voteStart_wrap .detailBox .label_box .border {
  width: 100%;
  position: absolute;
  right: 0;
  bottom: 0;
  border-bottom: 3rpx solid #343a40;
  height: 230rpx;
  z-index: 0;
}
Page .voteStart_wrap .detailBox .label_box .animation {
  animation-name: border-left-bottom;
  animation-duration: 0.5s;
  animation-timing-function: ease-out;
  animation-fill-mode: both;
}
Page .voteStart_wrap .detailBox .label_box_textarea {
  border-bottom: 3rpx solid #343a40;
}
Page .voteStart_wrap .detailBox .textarea {
  height: 20%;
  transition: all 0.3s ease-out;
  border-bottom: 0;
}
Page .voteStart_wrap .detailBox .textarea .label_textarea {
  border: 3rpx solid #006494;
  border-radius: 10rpx;
  position: absolute;
  width: calc(100% - 26rpx);
  bottom: 0;
  height: 160rpx;
  font-size: 26rpx;
  padding: 10rpx;
}
Page .voteStart_wrap .detailBox .textarea .title_on {
  font-size: 30rpx;
  color: #006494;
  transition: all 0.3s ease-out;
  transform: translateY(-180rpx);
}
Page .voteStart_wrap .detailBox .textarea .border {
  border-bottom: 3rpx solid #006494;
  transition: all 0.3s ease-out;
}
Page .voteStart_wrap .detailBox .label_boxs {
  height: 9%;
  width: 90%;
  position: relative;
  margin-top: 15rpx;
  overflow: hidden;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
Page .voteStart_wrap .detailBox .label_boxs .label_boxs_item {
  height: 100%;
  width: 45%;
  position: relative;
  overflow: hidden;
  z-index: 50;
}
Page .voteStart_wrap .detailBox .label_boxs .label_boxs_item .title {
  position: absolute;
  bottom: 20rpx;
  font-size: 36rpx;
  left: 0;
  transition: all 0.3s ease-out;
  color: #343a40;
}
Page .voteStart_wrap .detailBox .label_boxs .label_boxs_item .title_on {
  font-size: 30rpx;
  color: #006494;
  transition: all 0.3s ease-out;
  transform: translateY(-50rpx);
}
Page .voteStart_wrap .detailBox .label_boxs .label_boxs_item .label_input {
  height: 50%;
  width: 100%;
  position: absolute;
  bottom: 8rpx;
  z-index: 20;
  font-size: 36rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
Page .voteStart_wrap .detailBox .label_boxs .label_boxs_item .border {
  width: 100%;
  position: absolute;
  right: 0;
  bottom: 0;
  border-bottom: 3rpx solid #343a40;
  height: 230rpx;
  z-index: 0;
}
Page .voteStart_wrap .detailBox .label_boxs .label_boxs_item .animation {
  animation-name: border-left-bottom;
  animation-duration: 0.5s;
  animation-timing-function: ease-out;
  animation-fill-mode: both;
}
Page .voteStart_wrap .detailBox .options_box {
  height: 27%;
  width: 90%;
  position: relative;
  margin-top: 15rpx;
  overflow-y: scroll;
  overflow-x: hidden;
  display: flex;
  align-items: flex-start;
  flex-wrap: wrap;
  align-content: flex-start;
}
Page .voteStart_wrap .detailBox .options_box .addIcon {
  height: 33%;
  width: 45%;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  border-bottom: 3rpx solid #343a40;
  position: relative;
}
Page .voteStart_wrap .detailBox .options_box .addIcon .icon-jiahao {
  position: absolute;
  top: 50%;
  font-size: 38rpx;
  color: #3399FF;
  background: white;
  height: 30rpx;
  width: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  right: 5rpx;
}
Page .voteStart_wrap .detailBox .options_box .addIcon .addText {
  font-size: 34rpx;
  color: #343a40;
  margin-bottom: 20rpx;
}
Page .voteStart_wrap .detailBox .options_box .label_boxs_item {
  height: 33%;
  width: 45%;
  position: relative;
  overflow: hidden;
  z-index: 50;
}
Page .voteStart_wrap .detailBox .options_box .label_boxs_item:nth-child(2n) {
  margin-left: 10%;
}
Page .voteStart_wrap .detailBox .options_box .label_boxs_item .jianhao_box {
  position: absolute;
  right: 5rpx;
  top: 50%;
  background: white;
  height: 30rpx;
  width: 30rpx;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 9999;
}
Page .voteStart_wrap .detailBox .options_box .label_boxs_item .jianhao_box .icon-jianhao1 {
  font-size: 40rpx;
  color: #f4a261;
}
Page .voteStart_wrap .detailBox .options_box .label_boxs_item .title {
  position: absolute;
  bottom: 20rpx;
  font-size: 36rpx;
  left: 0;
  transition: all 0.3s ease-out;
  color: #343a40;
}
Page .voteStart_wrap .detailBox .options_box .label_boxs_item .title_on {
  font-size: 30rpx;
  color: #006494;
  transition: all 0.3s ease-out;
  transform: translateY(-50rpx);
}
Page .voteStart_wrap .detailBox .options_box .label_boxs_item .label_input {
  height: 50%;
  width: 100%;
  position: absolute;
  bottom: 8rpx;
  z-index: 20;
  font-size: 36rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
Page .voteStart_wrap .detailBox .options_box .label_boxs_item .border {
  width: 100%;
  position: absolute;
  right: 0;
  bottom: 0;
  border-bottom: 3rpx solid #343a40;
  height: 230rpx;
  z-index: 0;
}
Page .voteStart_wrap .detailBox .options_box .label_boxs_item .animation {
  animation-name: border-left-bottom;
  animation-duration: 0.5s;
  animation-timing-function: ease-out;
  animation-fill-mode: both;
}
@keyframes border-left-bottom {
  0% {
    width: 100%;
    border-bottom: 3rpx solid #343a40;
  }
  25% {
    width: 93%;
    border-bottom-left-radius: 15%;
  }
  50% {
    width: 86%;
    border-bottom-left-radius: 30%;
  }
  75% {
    width: 93%;
    border-bottom-left-radius: 15%;
  }
  100% {
    width: 100%;
    border-bottom-left-radius: 0;
    border-bottom: 3rpx solid #006494;
  }
}
Page .voteStart_wrap .publish_btn {
  position: fixed;
  margin: auto;
  left: 0;
  right: 0;
  bottom: 6%;
  width: 50%;
  height: 80rpx;
  margin-bottom: 20rpx;
  border-radius: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  letter-spacing: 2rpx;
  background: rgba(0, 129, 255, 0.8);
  color: white;
  font-weight: 100!important;
}
Page .voteStart_wrap .publish_btn:active {
  opacity: 0.8;
}

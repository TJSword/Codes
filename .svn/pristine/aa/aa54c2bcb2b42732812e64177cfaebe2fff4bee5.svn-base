        .nav{
            position:fixed;
            margin:auto;
            left:0;
            right:0;
            bottom:5%;
            border-radius: 50%;
            height:76rpx;
            width:76rpx;
            display: flex;
            justify-content: center;
            align-items: center;
            transition: all 0.4s ease-out;
            &::after{
                content:"";
                display: block;
                box-shadow:-10rpx 20rpx 20rpx 20rpx rgba(0, 0, 0, 0.3);
            }
            .hexagon {
                display: inline-block;
                
            }
            .hexagon .bar-rotate-left {
                width:80rpx;
                height:93rpx;
                overflow: hidden;
                transform: rotate(60deg);
                display: inline-block;
            }
    
            .hexagon .bar-rotate-right {
                width:80rpx;
                height:93rpx;
                overflow: hidden;
                transform: rotate(60deg);
                display: inline-block;
                
            }
    
            .hexagon .bar-horizontal {
                width:80rpx;
                height:93rpx;
                background: linear-gradient(to top, #1a2980, #26d0ce); /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */
                transform: rotate(-120deg);
                display: flex;
                justify-content: center;
                align-items: center;
                .icon-caidan{
                    color:white;
                    font-size:36rpx;
                }
            }
        }
        .nav_active{
            transform: scale(50);
            background: rgba(0, 0, 0, 0.3);
            transition: all 0.4s ease-out;
        }

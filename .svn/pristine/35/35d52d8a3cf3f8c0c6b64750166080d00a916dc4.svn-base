/* pages/canvasDemo/canvasDemo.wxss */
Page{
  height:100%;
  width:100%;
  overflow: hidden;
}
.progeress_wrap{
  width:100%;
  height:100%;
  background: #EEEEEE;
  overflow: hidden;
}
.title_box{
  margin-left:32rpx;
  height: 45px;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.progress_title{
  font-size: 38rpx;
  color: #2b3e43;
  font-family: pingfang;
  font-weight: 400;
}
.canvas_box{
  width:100%;
  height:80.8%;
}
.bottom_box{
  width: 100%;
  height:8%;
  background: #EEEEEE;
  display: flex;
  position: relative;
  z-index:auto
}
.left_box{
  width:60%;
  height:100%;
  font-size:24rpx;
  margin-left:7%;
  color:rgb(167, 164, 164);
}
.right_box{
  width:40%;
  height:100%;
  font-size:24rpx;
  display: flex;
  justify-content: center;
}
.btn{
  width: fit-content;
  height: fit-content;
  padding:20rpx;
  border-radius: 20rpx;
  border:1rpx solid rgb(167, 164, 164);
  margin-top: 10rpx;
  color:rgb(167, 164, 164)
}